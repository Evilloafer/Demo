@model Demo.Models.userInfo

@{
    ViewBag.Title = "Delete";
}

<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/jquery-3.2.1.min.js"></script>
<script type="text/javascript">
    function back() {
        window.location.href = "/User/ShowUserInfo";
    }
</script>

<table class="table table-striped" style="margin: 0 auto;width: 400px">
    <caption><h2>Are you sure you want to delete this?</h2></caption>
    <tr>
        <td>用户名</td>
        <td>密码</td>
    </tr>
    <tr>
        <td>@Html.DisplayFor(model => model.name)</td>
        <td>@Html.DisplayFor(model => model.pwd)</td>
    </tr>
</table>
@using(Html.BeginForm()){
    <input type="submit" value="删除" class="btn btn-info btn-lg btn-block" style="margin: 0 auto;width: 400px"/>
}
<p>
    <button class="btn btn-default btn-lg btn-block" onclick="back()" style="margin: 0 auto;width: 400px">返回</button> 
</p>
